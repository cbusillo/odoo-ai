<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_return_method_list" model="ir.ui.view">
        <field name="name">school.return.method.list</field>
        <field name="model">school.return.method</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="external_key"/>
                <field name="sequence"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="view_return_method_form" model="ir.ui.view">
        <field name="name">school.return.method.form</field>
        <field name="model">school.return.method</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="partner_id"/>
                            <field name="external_key"/>
                            <field name="sequence"/>
                            <field name="active"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
