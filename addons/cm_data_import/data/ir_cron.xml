<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ir_cron_cm_data_import" model="ir.cron">
        <field name="name">CM Data Import</field>
        <field name="model_id" ref="model_integration_cm_data_importer"/>
        <field name="state">code</field>
        <field name="code">model.run_scheduled_import()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number" eval="15"/>
        <field name="interval_type">minutes</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
