<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_location_option_alias_list" model="ir.ui.view">
        <field name="name">school.location.option.alias.list</field>
        <field name="model">school.location.option.alias</field>
        <field name="arch" type="xml">
            <list>
                <field name="location_option_id"/>
                <field name="system_id"/>
                <field name="external_key"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="view_location_option_alias_form" model="ir.ui.view">
        <field name="name">school.location.option.alias.form</field>
        <field name="model">school.location.option.alias</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="location_option_id"/>
                            <field name="system_id"/>
                        </group>
                        <group>
                            <field name="external_key"/>
                            <field name="active"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
