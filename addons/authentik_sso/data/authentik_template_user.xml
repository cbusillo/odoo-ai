<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="authentik_template_user" model="res.users">
            <field name="name">Authentik Template User</field>
            <field name="login">authentik_template_user</field>
            <field name="active" eval="True"/>
            <field name="share" eval="False"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="company_ids" eval="[(6, 0, [ref('base.main_company')])]"/>
            <field name="group_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
        </record>
    </data>
</odoo>
